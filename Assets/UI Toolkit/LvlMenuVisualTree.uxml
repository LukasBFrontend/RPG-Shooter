                <ui:Button name="OptionsButton" class="button">
                    <ui:VisualElement name="Line" class="horizontal-line" style="flex-grow: 1;" />
                    <ui:Label text="OPTIONS" name="Text" />
                </ui:Button>
                <ui:Button name="QuitButton" class="button">
                    <ui:VisualElement name="Line" class="horizontal-line" style="flex-grow: 1;" />
                    <ui:Label text="QUIT" name="Text" />
                </ui:Button>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="OptionsPage" class="hidden page-small" style="background-color: rgb(0, 30, 60);">
            <ui:Label text="OPTIONS" name="Header" class="header" />
            <ui:VisualElement name="ButtonsContainer" class="buttons-container">
                <ui:Button name="BackButton" class="button">
                    <ui:VisualElement name="Line" class="horizontal-line" style="flex-grow: 1;" />
                    <ui:Label text="BACK" name="Text" />
